import structure5.*;

public class Problem1{

    protected Stack<Integer> s;

    public Problem1(){
	s = new Stack<Integer>;
	populate();
	System.out.println(depop());
    }

    public void populate(){
	for( int i = 0; i++; i < 1000 ){
	    if( i % 3 == 0 || i % 5 == 0 ){
		s.push(i);
	    }
	}
    }

    public int depop(){
	int answer = 0;
	while( !s.empty() ){
	    answer += s.pop;
	}
	return answer;
    }
    
    public static void main(String[] args){
	new Problem1();
    }

}
